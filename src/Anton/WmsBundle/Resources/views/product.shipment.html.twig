{% extends "@AntonWms/admin.base.html.twig" %}
{% block sonata_head_title %}Отгрузка товара{% endblock %}
{% block content %}
    <label for="product-mapped">Товары на складах</label>
    <select id="product-mapped">
        <option>Не выбрано</option>
        {% for product in products %}
            <option value="{{ product.id }}">{{ product.name }} : {{ product.barcode }}</option>
        {% endfor %}
    </select>
    <button id="product__shipment" class="uk-button uk-button-primary uk-width-1-1 uk-margin-small-bottom">Отгрузить</button>
{% endblock %}